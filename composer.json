{
    "name": "alex-kalanis/email-smart-sender",
    "description": "Smart Sender mail service as part of EmailApi",
    "keywords": [
        "email",
        "smart-sender",
        "smart sender"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "alex-kalanis/email-api": "2.0"
    },
    "require-dev": {
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\EmailSmartSender\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
